@{
}

@model Book


<div class="card m-1 p-1">
    <ul>
        <li>@Model.Title</li>
        <li>@Model.Author</li>
        <li>@Model.Publisher</li>
        <li>@Model.Isbn</li>
        <li>@Model.Classification</li>
        <li>@Model.Category</li>
        <li>@Model.PageCount</li>
        <li>@Model.Price</li>
    </ul>
</div>